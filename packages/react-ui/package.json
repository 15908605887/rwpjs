{
  "name": "@rwp/react-ui",
  "version": "0.0.49-alpha.0",
  "license": "AGPL-3.0",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "release": "npm run build && npm publish",
    "lint:script": "eslint ./src/**/*.{js,jsx,ts,tsx}",
    "lint:fix": "eslint ./src/**/*.{js,jsx,ts,tsx} --fix",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:watch": "umi-test --watch --coverage",
    "test:coverage": "umi-test --coverage"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:script && npm run test && npm run build"
    }
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.1",
    "antd": "^4.3.5",
    "lodash": "^4.17.15",
    "react": "^16.12.0",
    "react-data-grid-temp": "^7.0.0-canary.17.1"
  },
  "devDependencies": {
    "@testing-library/react": "^10.3.0",
    "@types/jest": "^26.0.0",
    "@umijs/preset-react": "^1.x",
    "@umijs/test": "^3.0.5",
    "dumi": "^1.0.31",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.9.0"
  },
  "bugs": {
    "url": "https://github.com/jhoneybee/rwpjs/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jhoneybee/rwpjs"
  }
}
